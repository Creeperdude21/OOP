public class Main {
  public static void main(String[] args) {
    Person myObj = new Person();
    myObj.setName("prashant"); 
    System.out.println("The hidden name was " + myObj.getName());
  }
}

class Person {
  private String name; 

  
  public String getName() {
    return name;
  }

  
public void setName(String newName) {
    this.name = newName;
  }
}
